{
  "name": "tiny-jsx",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": {
    "tiny-jsx": "dist/tiny-jsx.umd.js",
    "tiny-jsx-dom": "dist/tiny-jsx-dom.umd.js"
  },
  "source": "src/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stanchino/tiny-jsx.git"
  },
  "scripts": {
    "watch": "rimraf dist && rollup -c --watch",
    "build": "rimraf dist && yarn rollup -c",
    "dev:server": "webpack-dev-server",
    "dev": "run-p watch dev:server",
    "run": "yarn build && NODE_ENV=production webpack --mode production && NODE_ENV=production static -z -p 5000 build",
    "pub": "cd dist && npm publish"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.14.2",
    "rollup-plugin-alias": "^1.5.2",
    "rollup-plugin-analyzer": "^3.0.1",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-copy": "^2.0.1",
    "rollup-plugin-gzip": "^2.2.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-terser": "^5.0.0",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.32.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1",
    "yarn-run-all": "^3.1.1"
  },
  "dependencies": {
    "mitt": "^1.1.3"
  }
}
